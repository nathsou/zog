let showNum: num => str = raw { n => String(n) }

trait Show {
    fun show(self: Self): str
}

impl Show for num {
    fun show(n) {
        showNum(n) 
    }
}

impl Show for str {
    fun show(s) {
        s 
    }
}

fun f() {
    3.show()
}

