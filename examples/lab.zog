
let range = iterator (start, end, step) -> {
    mut i = start
    
    while i < end {
        yield i
        i += step
    }
}

let fact = n -> if n == 0 { 1 } else { n * fact(n - 1) }

for n in range(1, 17, 1) {
    print(fact(n))
}

