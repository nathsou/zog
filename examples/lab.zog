
declare console: { log: _ => () }

let showNum: num => str = raw { n => String(n) }

trait Show {
    fun show(self: Self): str
}

impl Show for num {
    fun show(n) {
        showNum(n) 
    }
}

impl Show for str {
    fun show(s) {
        s 
    }
}

fun print(x) {
    (console.log)(x.show())
}

fun id(x) {
    x
}

let _ = (id(1), id(true))

print(17)

