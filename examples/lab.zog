import "../std/array.zog" { map }
import "../std/string.zog" { join }

declare console: { log: _ => () }

trait Show {
    fun show(x: Self): str
}

impl Show for (a, b) {
    fun show((x, y) : (a, b)): str {
        "(" ++ x.show() ++ ", " ++ y.show() ++ ")"
    }
}

let showNum: num => str = raw { n => String(n) }

impl Show for num {
    fun show(n: num): str {
        showNum(n)
    }
}

impl Show for str {
    fun show(s: str): str {
        s
    }
}

(console.log)((23, "yo").show())